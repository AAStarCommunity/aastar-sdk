{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "packages/core",
    "packages/account",
    "packages/paymaster",
    "packages/tokens",
    "packages/community",
    "packages/identity",
    "packages/analytics",
    "packages/enduser",
    "packages/dapp"
  ],
  "entryPointStrategy": "packages",
  "out": "docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "exclude": ["**/node_modules/**", "**/dist/**", "**/tests/**"],
  "excludeExternals": true,
  "skipErrorChecking": true,
  "parametersFormat": "table",
  "typeDeclarationFormat": "list",
  "expandObjects": false,
  "readme": "none",
  "hidePageHeader": true,
  "hidePageTitle": true,
  "hideBreadcrumbs": true,
  "cleanOutputDir": true,
  "externalSymbolLinkMappings": {
    "viem": {
      "*": "https://viem.sh/docs/index.html"
    }
  }
}
