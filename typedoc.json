{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "packages/core",
    "packages/account",
    "packages/paymaster",
    "packages/tokens"
  ],
  "entryPointStrategy": "packages",
  "out": "docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "exclude": ["**/node_modules/**", "**/dist/**", "**/tests/**"],
  "excludeExternals": true,
  "skipErrorChecking": true,
  "readme": "none",
  "hidePageHeader": true,
  "hidePageTitle": true,
  "hideBreadcrumbs": true,
  "cleanOutputDir": true,
  "logLevel": "Error",
  "validation": {
    "notDocumented": false,
    "invalidLink": false,
    "notExported": false
  },
  "compilerOptions": {
    "skipLibCheck": true,
    "composite": false,
    "paths": {
      "@aastar/core": ["./packages/core/src"],
      "@aastar/core/*": ["./packages/core/src/*"],
      "@aastar/account": ["./packages/account/src"],
      "@aastar/account/*": ["./packages/account/src/*"],
      "@aastar/paymaster": ["./packages/paymaster/src"],
      "@aastar/paymaster/*": ["./packages/paymaster/src/*"],
      "@aastar/tokens": ["./packages/tokens/src"],
      "@aastar/tokens/*": ["./packages/tokens/src/*"]
    }
  },
  "externalSymbolLinkMappings": {
    "viem": {
      "*": "https://viem.sh/docs/index.html"
    }
  }
}
