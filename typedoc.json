{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "packages/core",
    "packages/account",
    "packages/paymaster",
    "packages/tokens",
    "packages/analytics",
    "packages/dapp",
    "packages/identity",
    "packages/sdk"
  ],
  "entryPointStrategy": "packages",
  "out": "docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "exclude": ["**/node_modules/**", "**/dist/**", "**/tests/**"],
  "excludeExternals": true,
  "skipErrorChecking": true,
  "readme": "none",
  "hidePageHeader": true,
  "hidePageTitle": true,
  "hideBreadcrumbs": true,
  "cleanOutputDir": true,
  "logLevel": "Info",
  "parametersFormat": "Table",
  "typeDeclarationFormat": "Table",
  "useCodeBlocks": true,
  "expandObjects": false,
  "validation": {
    "notDocumented": false,
    "invalidLink": false,
    "notExported": false
  },
  "compilerOptions": {
    "skipLibCheck": true,
    "composite": false,
    "paths": {
      "@aastar/core": ["./packages/core/src"],
      "@aastar/core/*": ["./packages/core/src/*"],
      "@aastar/account": ["./packages/account/src"],
      "@aastar/account/*": ["./packages/account/src/*"],
      "@aastar/paymaster": ["./packages/paymaster/src"],
      "@aastar/paymaster/*": ["./packages/paymaster/src/*"],
      "@aastar/tokens": ["./packages/tokens/src"],
      "@aastar/tokens/*": ["./packages/tokens/src/*"],
      "@aastar/analytics": ["./packages/analytics/src"],
      "@aastar/analytics/*": ["./packages/analytics/src/*"],
      "@aastar/dapp": ["./packages/dapp/src"],
      "@aastar/dapp/*": ["./packages/dapp/src/*"],
      "@aastar/identity": ["./packages/identity/src"],
      "@aastar/identity/*": ["./packages/identity/src/*"],
      "@aastar/sdk": ["./packages/sdk/src"],
      "@aastar/sdk/*": ["./packages/sdk/src/*"]
    }
  },
  "externalSymbolLinkMappings": {
    "viem": {
      "*": "https://viem.sh/docs/"
    }
  }
}
