{
  "name": "@aastar/core",
  "version": "0.16.6",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "find src -name '*.js' -o -name '*.d.ts' -o -name '*.map' | xargs rm -f",
    "build": "pnpm clean && tsc",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint src --ext .ts",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint src --ext .ts --fix",
    "audit:abi": "npx tsx scripts/audit-abi-coverage.ts"
  },
  "dependencies": {
    "@noble/curves": "^1.6.0",
    "viem": "2.43.3"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "4.0.17",
    "abitype": "1.2.3",
    "typescript": "5.7.2",
    "vitest": "4.0.17"
  },
  "publishConfig": {
    "access": "public"
  }
}
