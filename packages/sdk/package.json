{
  "name": "@aastar/sdk",
  "version": "0.16.6",
  "description": "AAStar SDK - The all-in-one package for Mycelium Network",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@aastar/account": "workspace:*",
    "@aastar/core": "workspace:*",
    "@aastar/dapp": "workspace:*",
    "@aastar/identity": "workspace:*",
    "@aastar/paymaster": "workspace:*",
    "@aastar/tokens": "workspace:*",
    "viem": "2.43.3"
  },
  "devDependencies": {
    "typescript": "5.7.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/node/index.js"
    }
  },
  "scripts": {
    "clean": "find src -name '*.js' -o -name '*.d.ts' -o -name '*.map' | xargs rm -f",
    "prebuild": "pnpm clean",
    "build": "tsc",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint src --ext .ts",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint src --ext .ts --fix",
    "example:local": "tsx examples/regression_test.ts"
  },
  "license": "MIT"
}
